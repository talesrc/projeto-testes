{
    "compilerOptions": {
        "sourceMap": true,
        "outDir": "dist",
        "strict": true,
        "lib": [
            "es2015"
        ],
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "target": "es6",
        "moduleResolution": "Node",
        "strictPropertyInitialization": false,
        "noImplicitAny": false,
        "skipLibCheck": true
    }
}